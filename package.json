{
  "name": "portfolio",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.1",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "eslint": "8.57.1",
    "jest": "^29.6.1",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0"
  },
  "scripts": {
    "postinstall": "patch-package",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "lint": "eslint \"src/**/*.{js,jsx}\" || echo \"Lint failed or not installed\"",
    "serve:prod": "http-server build -p 5000 -a 0.0.0.0 -c-1",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop ecosystem.config.js || true",
    "pm2:restart": "pm2 restart ecosystem.config.js --env production",
    "pm2:status": "pm2 ls",
    "pm2:save": "pm2 save",
    "eject": "react-scripts eject"
  },
  "overrides": {
    "nth-check": "2.0.1",
    "postcss": "8.4.31",
    "svgo": "2.8.0",
    "webpack-dev-server": "5.2.2",
    "css-select": "4.1.3"
  }
}
